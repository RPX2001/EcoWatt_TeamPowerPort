        "name": "JSON Extractor",
        "func": "// let device = msg.payload.device_id;\n// let entries = msg.payload.entries;\n\n// let vac1 = [];\n// let iac1 = [];\n// let ipv1 = [];\n// let pac = [];\n\n// for (let entry of entries) {\n//     let ts = entry.timestamp;\n\n//     vac1.push({\n//         topic: device + \"/REG_VAC1\",\n//         payload: {\n//             value: entry.register_mapping.REG_VAC1,\n//             timestamp: ts\n//         }\n//     });\n\n//     iac1.push({\n//         topic: device + \"/REG_IAC1\",\n//         payload: {\n//             value: entry.register_mapping.REG_IAC1,\n//             timestamp: ts\n//         }\n//     });\n\n//     ipv1.push({\n//         topic: device + \"/REG_IPV1\",\n//         payload: {\n//             value: entry.register_mapping.REG_IPV1,\n//             timestamp: ts\n//         }\n//     });\n\n//     pac.push({\n//         topic: device + \"/REG_PAC\",\n//         payload: {\n//             value: entry.register_mapping.REG_PAC,\n//             timestamp: ts\n//         }\n//     });\n// }\n\n// // return as 4 outputs → wire each to its own Chart\n// return [msg,vac1, iac1, ipv1, pac];\n\n// Register mapping from your table\nconst data = msg.payload.register_data;\n\n// Calculate actual values with units and formatting\nconst acVoltage = data.REG_VAC1 ? {\n    payload: data.REG_VAC1 / 10,\n    unit: \"V\",\n    formatted: (data.REG_VAC1 / 10).toFixed(1) + \"V\"\n} : null;\n\nconst acCurrent = data.REG_IAC1 ? {\n    payload: data.REG_IAC1 / 10,\n    unit: \"A\",\n    formatted: (data.REG_IAC1 / 10).toFixed(2) + \"A\"\n} : null;\n\nconst acFrequency = data.REG_FAC1 ? {\n    payload: data.REG_FAC1 / 100,\n    unit: \"Hz\",\n    formatted: (data.REG_FAC1 / 100).toFixed(2) + \"Hz\"\n} : null;\n\nconst pv1Voltage = data.REG_VPV1 ? {\n    payload: data.REG_VPV1 / 10,\n    unit: \"V\",\n    formatted: (data.REG_VPV1 / 10).toFixed(1) + \"V\"\n} : null;\n\nconst pv2Voltage = data.REG_VPV2 ? {\n    payload: data.REG_VPV2 / 10,\n    unit: \"V\",\n    formatted: (data.REG_VPV2 / 10).toFixed(1) + \"V\"\n} : null;\n\nconst pv1Current = data.REG_IPV1 ? {\n    payload: data.REG_IPV1 / 10,\n    unit: \"A\",\n    formatted: (data.REG_IPV1 / 10).toFixed(2) + \"A\"\n} : null;\n\nconst pv2Current = data.REG_IPV2 ? {\n    payload: data.REG_IPV2 / 10,\n    unit: \"A\",\n    formatted: (data.REG_IPV2 / 10).toFixed(2) + \"A\"\n} : null;\n\nconst temperature = data.REG_TEMP ? {\n    payload: data.REG_TEMP / 10,\n    unit: \"°C\",\n    formatted: (data.REG_TEMP / 10).toFixed(1) + \"°C\"\n} : null;\n\nconst exportPower = data.REG_EXPORT_POWER ? {\n    payload: data.REG_EXPORT_POWER,\n    unit: \"%\",\n    formatted: data.REG_EXPORT_POWER.toFixed(0) + \"%\"\n} : null;\n\nconst acPower = data.REG_PAC ? {\n    payload: data.REG_PAC,\n    unit: \"W\",\n    formatted: data.REG_PAC.toFixed(0) + \"W\"\n} : null;\n\n// Return array with 10 outputs\nreturn [\n    acVoltage,      // Output 1: AC Voltage with unit \"V\"\n    acCurrent,      // Output 2: AC Current with unit \"A\"  \n    acFrequency,    // Output 3: AC Frequency with unit \"Hz\"\n    pv1Voltage,     // Output 4: PV1 Voltage with unit \"V\"\n    pv2Voltage,     // Output 5: PV2 Voltage with unit \"V\"\n    pv1Current,     // Output 6: PV1 Current with unit \"A\"\n    pv2Current,     // Output 7: PV2 Current with unit \"A\"\n    temperature,    // Output 8: Temperature with unit \"°C\"\n    exportPower,    // Output 9: Export Power with unit \"%\"\n    acPower         // Output 10: AC Power with unit \"W\"\n];\n",
